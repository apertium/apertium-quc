DELIMITERS = "<.>" "<!>" "<;>" "<?>" ;

LIST A = adj ;
LIST N = n ;
LIST Clf = clf ;
LIST Prop = np ;
LIST Det = det ;

LIST Poss = px1sg px2sg px3sg px1pl px2pl px3pl ;

LIST @det = @det ;

SECTION

SELECT A IF (0C A OR N) (NOT 0 Poss) (1C N) ;

SELECT Clf IF (1C Prop) ;

SECTION

MAP @det TARGET Det IF (1C Clf LINK 1 N) ;
